{{#partial "contenido"}}
  


  <div class="py-5">
    <div class="container">
      <div class="form-group">
        <h4> Evaluacion de empresas </h4>
        <div class="row">
        
          <div class="col-md-4"> <label for="sel1" class="">Seleccione indicador:</label> <select class="form-control" name="selIndicador" id="sel1">
          {{#each indicadores}}
                        <option value="{{id}}">{{nombre}}</option>
           {{/each}}
                      </select> 
                     </div>
          <div class="col-md-4"> <label for="sel2" class="">Seleccione empresa:</label> <select class="form-control" name="selEmpresa" id="sel2">
                {{#each empresas}}
                        <option value="{{id}}">{{nombre}}</option>
           		{{/each}}
                      </select> </div>
          <div class="col-md-4">
          <form id="evaluarEmpresa" action="" method="POST">
            <button class="btn btn-primary btn-lg btn-block my-4" >Evaluar empresa </button>
            </form>
          </div>
        </div>
        
        
        
     


        </div>
      </div>
    </div>
  </div>
  
  

  
{{/partial}}

{{>layout.hbs}}